{
    "name": "impiger/widget",
    "description": "Widget package for Impiger CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Impiger\\Widget\\": "src"
        }
    },
    "require": {
        "impiger/platform": "*@dev",
        "impiger/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Impiger\\Widget\\Providers\\WidgetServiceProvider"
            ],
            "aliases": {
                "Widget": "Impiger\\Widget\\Facades\\WidgetFacade",
                "WidgetGroup": "Impiger\\Widget\\Facades\\WidgetGroupFacade"
            }
        }
    }
}
