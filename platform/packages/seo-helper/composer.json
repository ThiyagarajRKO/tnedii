{
    "name": "impiger/seo-helper",
    "description": "SEO helper package for Impiger CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Impiger\\SeoHelper\\": "src"
        }
    },
    "require": {
        "impiger/platform": "*@dev",
        "impiger/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Impiger\\SeoHelper\\Providers\\SeoHelperServiceProvider"
            ],
            "aliases": {
                "SeoHelper": "Impiger\\SeoHelper\\Facades\\SeoHelperFacade"
            }
        }
    }
}
