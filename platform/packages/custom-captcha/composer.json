{
	"name": "impiger/custom-captcha",
	"type": "package",
	"description": "Laravel 5 & 6 CustomCaptcha Package",
	"keywords": ["laravel6 CustomCaptcha", "laravel6 Security", "laravel6 CustomCaptcha", "laravel5 Security", "CustomCaptcha"],
	"homepage": "https://github.com/mewebstudio/customcaptcha",
	"license": "MIT",
	"authors": [
		{
			"name": "Muharrem ERÄ°N",
			"email": "me@mewebstudio.com",
			"homepage": "https://github.com/mewebstudio",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.2|^8.0",
		"ext-gd": "*",
		"illuminate/config": "~5|^6|^7|^8",
		"illuminate/filesystem": "~5|^6|^7|^8",
		"illuminate/support": "~5|^6|^7|^8",
		"illuminate/hashing": "~5|^6|^7|^8",
		"illuminate/session": "~5|^6|^7|^8",
		"intervention/image": "~2.5"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5",
		"mockery/mockery": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Impiger\\CustomCaptcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		],
		"psr-4": {
			"Impiger\\Test\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Impiger\\CustomCaptcha\\CustomCaptchaServiceProvider"
			],
			"aliases": {
				"CustomCaptcha": "Impiger\\CustomCaptcha\\Facades\\CustomCaptcha"
			}
		}
	}
}
