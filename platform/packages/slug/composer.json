{
    "name": "impiger/slug",
    "description": "Slug/Permalink for Impiger CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Impiger\\Slug\\": "src"
        }
    },
    "require": {
        "impiger/platform": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Impiger\\Slug\\Providers\\SlugServiceProvider"
            ],
            "aliases": {
                "SlugHelper": "Impiger\\Slug\\Facades\\SlugHelperFacade"
            }
        }
    }
}
