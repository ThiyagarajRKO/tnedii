{
    "name": "impiger/dev-tool",
    "description": "Dev tools package for Impiger CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Impiger\\DevTool\\": "src"
        }
    },
    "require": {
        "impiger/platform": "*@dev",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "squizlabs/php_codesniffer": "^3.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "phpmd/phpmd": "^2.8",
        "doctrine/dbal": "^3.0",
        "impiger/git-commit-checker": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Impiger\\DevTool\\Providers\\DevToolServiceProvider"
            ]
        }
    }
}
