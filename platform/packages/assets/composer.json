{
    "name": "impiger/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "impiger",
        "impiger cms",
        "impiger platform",
        "impiger assets"
    ],
    "homepage": "https://impigertech.com",
    "authors": [
        {
            "name": "Impiger Technologies",
            "email": "vijayaragavan.ambalam@impigertech.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "laravel/framework": "^5.6|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Impiger\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Impiger\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Impiger\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "Impiger\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
